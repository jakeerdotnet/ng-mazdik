{
  "name": "ng-mazdik",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-styles": "scss-bundle",
    "lib:build": "ng build ng-mazdik-lib --configuration production && npm run build-styles",
